{
  "name": "sources-ui",
  "version": "1.1.0",
  "private": false,
  "insights": {
    "appname": "sources"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatInsights/sources-ui.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@data-driven-forms/pf4-component-mapper": "^2.23.3",
    "@data-driven-forms/react-form-renderer": "^2.23.3",
    "@patternfly/react-core": "^4.97.2",
    "@patternfly/react-icons": "^4.9.2",
    "@patternfly/react-table": "^4.23.2",
    "@redhat-cloud-services/frontend-components": "3.0.8",
    "@redhat-cloud-services/frontend-components-notifications": "3.0.4",
    "@redhat-cloud-services/frontend-components-utilities": "3.0.3",
    "@sentry/minimal": "6.1.0",
    "axios": "0.21.1",
    "awesome-debounce-promise": "^2.1.0",
    "classnames": "^2.2.6",
    "deep-object-diff": "^1.1.0",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.12.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/stories/*",
      "!src/entry.js",
      "!src/entry-dev.js",
      "!src/frontend-components-copies/*",
      "!src/bootstrap.js",
      "!src/bootstrap-dev.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@patternfly|@data-driven-forms)).*$"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@redhat-cloud-services/frontend-components-config": "^4.0.8",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "webpack": "^5.22.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "whatwg-fetch": "^3.5.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "server:ctr": "node src/server/generateServerKey.js",
    "travis:verify": "npm-run-all lint test build:prod",
    "verify": "npm-run-all build lint test",
    "extract:messages": "npx @formatjs/cli extract 'src/**/*.{js,jsx}' --out-file ./translations/messages.json",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "prod:beta": "NODE_ENV=production BETA=true webpack serve --config config/dev.webpack.config.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js",
    "build": "webpack --config config/prod.webpack.config.js",
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js"
  }
}
